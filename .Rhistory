library(dplyr)
mecha_table <- read.csv("MechaCar_mpg.csv")
head(mecha_table)
library(ggplot2)
lm(mpg ~ vehicle_weight,data=mecha_table)
summary(lm(mpg ~ vehicle_weight,data=mecha_table))
model <- lm(mpg ~ vehicle_weight,data=mecha_table)
yvals <- model$coefficients["mpg"]*mecha_table$mpg + model$coefficients["(Intercept)"]
plt <- ggplot(mecha_table,aes(x=vehicle_weight,y=mpg))
plt + geom_point()
plt + geom_point() + geom_line(aes(y=yvals), color = "red")
